<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import org.kordamp.ikonli.javafx.FontIcon?>
<?import java.lang.*?>
<VBox prefHeight="300.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER_LEFT" spacing="5.0" styleClass="bg-inset">
         <children>
            <Label styleClass="title-4" text="MediaThor Downloader" />
            <Pane HBox.hgrow="ALWAYS" />
            <HBox>
               <children>
                  <Button contentDisplay="GRAPHIC_ONLY" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="32.0" prefWidth="48.0">
                     <graphic>
                        <FontIcon iconLiteral="mdi2w-window-minimize" />
                     </graphic>
                     <styleClass>
                        <String fx:value="left-pill" />
                        <String fx:value="flat" />
                     </styleClass>
                  </Button>
                  <Button contentDisplay="GRAPHIC_ONLY" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="32.0" prefWidth="48.0">
                     <graphic>
                        <FontIcon iconLiteral="mdi2w-window-maximize" />
                     </graphic>
                     <styleClass>
                        <String fx:value="center-pill" />
                        <String fx:value="flat" />
                     </styleClass>
                  </Button>
                  <Button contentDisplay="GRAPHIC_ONLY" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="32.0" prefWidth="48.0" style="-fx-border-radius: 0; -fx-background-radius: 0;">
                     <graphic>
                        <FontIcon iconLiteral="mdi2w-window-close" />
                     </graphic>
                     <styleClass>
                        <String fx:value="right-pill" />
                        <String fx:value="danger" />
                     </styleClass>
                  </Button>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets left="5.0" />
         </padding>
      </HBox>
      <VBox VBox.vgrow="ALWAYS" />
   </children>
</VBox>
